{
    "name": "battis/php-generator",
    "description": "Generate PHP code dynamically",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Seth Battis",
            "email": "seth@battis.net"
        }
    ],
    "require": {
        "battis/data-utilities": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Battis\\PHPGenerator\\": "src"
        }
